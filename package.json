{
  "name": "gyeonghwon",
  "version": "0.2.0",
  "author": "Code for History",
  "description": "Library for displaying animated image files in canvas context",
  "keywords": [
    "apng",
    "gif",
    "webp",
    "svg",
    "animation",
    "animated image",
    "canvas"
  ],
  "homepage": "https://github.com/code4history/Gyeonghwon",
  "license": "MIT",
  "type": "module",
  "main": "./dist/gyeonghwon.umd.cjs",
  "module": "./dist/gyeonghwon.js",
  "exports": {
    ".": {
      "import": "./dist/gyeonghwon.js",
      "require": "./dist/gyeonghwon.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && cross-env BUILD_MODE=package vite build",
    "deploy": "cp public/index.html index.html && tsc && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "generate:fixtures": "tsx scripts/generate-fixtures.ts"
  },
  "devDependencies": {
    "canvas": "^2.11.2",
    "gif-encoder": "^0.7.2",
    "maplibre-gl": "^3.6.2",
    "ol": "^8.2.0",
    "sharp": "^0.33.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "upng-js": "^2.1.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^1.6.0"
  }
}
